<div class="col-md-6">
	<h2>Projects</h2>
	<p><%= link_to "New", url_for(controller: :projects, action: :new) %></p>
	<ul>
		<% for project in @projects %>
			<li><%= project.title %></li>
		<% end %>
	</ul>
</div>
<div class="col-md-6">
	<h2>Users</h2>
	<p><%= link_to "New", url_for(controller: :users, action: :new) %></p>
	<ul>
		<% for user in @users %>
			<li><%= user.token %></li>
		<% end %>
	</ul>
</div>