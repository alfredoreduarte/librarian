<div class="col-md-6">
	<h2>Projects</h2>
	<p><%= link_to "New", url_for(controller: :projects, action: :new) %></p>
	<ul>
		<% for project in @projects %>
			<li><%= project.title %></li>
		<% end %>
	</ul>
	<hr />
	<h2>Articles</h2>
	<p><%= link_to "New", url_for(controller: :articles, action: :new) %></p>
	<ul>
		<% for article in @articles %>
			<li><%= link_to article.title, article %></li>
		<% end %>
	</ul>
</div>
<div class="col-md-6">
	<h2>Users</h2>
	<p><%= link_to "New", url_for(controller: :users, action: :new) %></p>
	<ul>
		<% for user in @users %>
			<li><%= link_to user.token, user %></li>
		<% end %>
	</ul>
</div>